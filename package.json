{
  "name": "typescript-backend-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
   "start": "tsc && ts-node -r tsconfig-paths/register ./build/app.js",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint.fix": "eslint . --ext .ts --fix",
    "m:generate": "sequelize-cli migration:generate --name",
    "m:run": "sequelize-cli db:migrate",
    "m:undo": "sequelize-cli db:migrate:undo",
    "m:delete": "sequelize-cli db:migrate:undo:all",
    "s:generate": "sequelize-cli seed:generate --name",
    "s:run": "sequelize-cli db:seed:all",
    "s:delete": "sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "*",
    "@types/stripe-v3": "^3.1.28",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "stripe": "^12.6.0",
    "tsc-alias": "^1.8.6",
    "zod": "^3.21.4"
  }
}
